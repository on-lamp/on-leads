# Notion API Integration Token
# Get it from https://www.notion.so/my-integrations
NOTION_TOKEN=your_notion_integration_token_here

# Google API Key for Gemini AI integration
# Get it from Google AI Studio (https://aistudio.google.com/)
GOOGLE_API_KEY=your_google_api_key_here

# Notion database ID for the Leads database
# Get it from the database URL: https://www.notion.so/{workspace}/{database_id}
NOTION_LEADS_DATABASE_ID=your_notion_leads_database_id_here

# Notion database ID for the Emails database
# Get it from the database URL: https://www.notion.so/{workspace}/{database_id}
NOTION_EMAILS_DATABASE_ID=your_notion_emails_database_id_here

# Firecrawl API key for web scraping
# Get it from your Firecrawl dashboard
FIRECRAWL_API_KEY=yout_firecrawl_api_key_here

OPENAI_API_KEY=your_openai_api_key_here

TELEGRAM_TOKEN=yout_telegram_bot_token_here